{
  "name": "simple-redux-utils",
  "version": "0.0.2",
  "description": "Simple Redux utils for facilitate workflow",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist lib",
    "prepublish": "npm run build",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore *.spec.js",
    "build:umd": "NODE_ENV=development webpack src/index.js dist/simple-redux-utils.js --progress",
    "build:umd:min": "NODE_ENV=production webpack src/index.js dist/simple-redux-utils.min.js --progress",
    "test": "npm run mocha \"./src/**/*.spec.js\"",
    "mocha": "mocha --require babel-register",
    "mocha:watch": "npm run mocha -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/simple-redux-utils.git"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "eslint": "3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.6.0",
    "mocha": "3.1.2",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-thunk": "2.1.0",
    "sinon": "1.17.6",
    "webpack": "1.13.3"
  }
}
